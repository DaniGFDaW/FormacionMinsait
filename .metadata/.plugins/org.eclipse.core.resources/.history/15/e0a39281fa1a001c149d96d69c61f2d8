package logica;

public class Docente extends Persona {

	private int notaMatematicas;
	private int notaBiologia;
	private int notaFisica;
	private int notaQuimica;


	public Docente(String nombre, int edad, int notas) {
		super(nombre, edad);
		this.notas = notas;
	}


	public int getNotas() {
		return this.notas;
	}


	public void setNotas(int notas) {
		this.notas = notas;
	}
	
	public void imprimir() {
		
		System.out.println("Nombre: " + this.getNombre() + "\nEdad: " + this.getEdad()
		+ "\nNota: " + this.getNotas());
	}
	
	
}
