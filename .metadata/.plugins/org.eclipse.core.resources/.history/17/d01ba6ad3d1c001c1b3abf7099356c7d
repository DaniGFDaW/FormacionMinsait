package Ejercicio1;

public class Usuario extends Cuenta{

	public Usuario() {
	}

	public Usuario(int numCuenta, String titularCuenta, double saldoCuenta, double interesAnualCuenta) {
		super(numCuenta, titularCuenta, saldoCuenta, interesAnualCuenta);
	}
 
	public void menuCliente() {
		
		String resultado;
		
		resultado = "----Menu Cliente----"+ "\nDepositar\n"
				+ "Girar\n"
				+ "Consulta Saldo\n" 
				+ "Convertir la moneda de Euro a USD\n"
				+ "Salir.";
		
		System.out.println(resultado);
	}
	
	
}
